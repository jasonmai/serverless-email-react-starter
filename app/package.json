{
  "name": "vite-react-recaptcha-app",
  "author": "Jason Mai",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:net": "vite --host",
    "dev:net:ssl": "export DEV_SSL=true && vite --host --port 5173",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "export VITE_CONTACT_API_URL=https://api.example.com && export VITE_RECAPTCHA_KEY=token && vitest run --coverage",
    "build": "tsc && vite build",
    "build:prod": "npm run lint && npm run test && npm run build",
    "analyze": "vite-bundle-visualizer -o visualize-bundle.html",
    "preview": "vite preview",
    "preview:net": "vite preview --host --port 5173"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.50.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-google-recaptcha": "^2.1.9",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "msw": "^2.2.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-bundle-visualizer": "^1.0.1",
    "vitest": "^1.3.0"
  }
}
